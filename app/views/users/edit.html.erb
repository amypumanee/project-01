<h1>Update your profile</h1>

<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :dob, "Date of birth" %>
  <%= f.date_field :dob %>

  <%= f.label :phone, "Mobile number" %>
  <%= f.text_field :phone %>

  <%= f.label :image %>
  <%= f.url_field :image %>


  <%= f.submit "Update profile" %>
  <% end %>
